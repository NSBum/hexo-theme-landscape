<% if (theme.links.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">Links</h3>
    <div class="widget">
        <% for (var i in theme.links){ %>
    		<li style="font-size:14px; list-style-type: none;">
    			<i class="<%- theme.links[i].icon %>"></i>
    			<a href="<%- theme.links[i].url %>" title="<%- theme.links[i].intro %>" ]);"><%= theme.links[i].title %></a></li>
    	<% } %>
    </div>
  </div>
<% } %>
